{
  "multipart": [
    { "when": { "axis": "x" }, "apply": { "model": "congregamystica:chain_brass_horizontal", "y": 90 } },
    { "when": { "axis": "y" }, "apply": { "model": "congregamystica:chain_brass" } },
    { "when": { "axis": "z" }, "apply": { "model": "congregamystica:chain_brass_horizontal" } },
    { "when": { "axis": "x", "dangle": "true" }, "apply": { "model": "congregamystica:chain_brass_dangle", "y": 90 } },
    { "when": { "axis": "y|z", "dangle": "true" }, "apply": { "model": "congregamystica:chain_brass_dangle" } }
  ]
}